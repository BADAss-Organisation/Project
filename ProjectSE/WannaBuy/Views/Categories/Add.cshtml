
﻿@model WannaBuy.ViewModels.Category.AddCategoryViewModel
<div class="row">
    <div class="col-sm-12 offset-lg-2 col-lg-8 offset-xl-3 col-xl-6">
        <form class="mt-5" method="post" action="Add">
            <div class="mb-3">
                <label class="form-label">Name</label>
